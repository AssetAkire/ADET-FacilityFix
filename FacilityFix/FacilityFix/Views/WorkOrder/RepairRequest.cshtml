@{
    ViewBag.Title = "Repair Request";
    ViewBag.ActiveMenu = "RepairRequest";
    string[] RequestFields = new string[] { "REQ No.", "Title", "Staff", "Date", "Status", "Actions" };
}

@section Styles {
    <link href="~/Content/TenantProfile.css" rel="stylesheet" />
}

<!-- Container for content -->
<div class="container my-5">
    <div class="d-flex justify-content-between align-items-center mb-4">
        <h2 class="fw-semibold">Repair Request</h2>

        <!-- Modal Trigger Button -->
        <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#repairRequestModal">
            Create Repair Request
        </button>
    </div>

    <!-- Request Details Section -->
    <div class="bg-white p-4 shadow-sm border border-2 rounded border-secondary p-3">
        <div class="row align-items-center mb-4">
            <div class="col-md-8 d-flex align-items-center">
                <div>
                    <h4 class="fw-semibold">Request Details</h4>
                    <p class="text-muted">Please provide the details of the repair request.</p>
                </div>
            </div>
        </div>

        <!-- Request Table -->
        <table class="table table-bordered align-middle">
            <thead class="table-light">
                <!-- Request Table Header -->
                <tr>
                    @{
                        foreach (var field in RequestFields)
                        {
                            <td>@field</td>
                        }
                    }
                </tr>
            </thead>
            <tbody>
                <!-- Detail Row -->
                <tr>
                    <td>RQ-001</td>
                    <td>Leaking Faucet</td>
                    <td>Juan Dela Cruz</td>
                    <td>2023-10-01</td>
                    <td><span class="badge bg-warning text-dark">In Progress</span></td>
                    <td>
                        <!-- Action Dropdown -->
                        <div class="dropdown">
                            <button class="btn btn-sm btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false" id="actionsDropdown">
                                Actions
                            </button>
                            <ul class="dropdown-menu" aria-labelledby="actionsDropdown">
                                <li>
                                    <a class="dropdown-item text-warning" href="#" data-bs-toggle="modal" data-bs-target="#viewModal">View</a>
                                </li>
                                <li>
                                    <a class="dropdown-item text-success" href="#" data-bs-toggle="modal" data-bs-target="#editModal">Edit</a>
                                </li>
                                <li>
                                    <a class="dropdown-item text-danger" href="#" data-bs-toggle="modal" data-bs-target="#deleteModal">Delete</a>
                                </li>
                            </ul>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>

    <!-- View Modal -->
            <div class="modal fade" id="viewModal" tabindex="-1" aria-labelledby="viewModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="viewModalLabel">View Request</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <p><strong>REQ No.:</strong> REQ-001</p>
                            <p><strong>Title:</strong> Leaking Faucet</p>
                            <p><strong>Staff:</strong> Juan Dela Cruz</p>
                            <p><strong>Date:</strong> 2023-10-01</p>
                            <p><strong>Status:</strong> <span class="badge bg-warning text-dark">In Progress</span></p>
                            <p><strong>Description:</strong> The faucet in the kitchen is leaking continuously.</p>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Edit Modal -->
            <div class="modal fade" id="editModal" tabindex="-1" aria-labelledby="editModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="editModalLabel">Edit Request</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <form>
                                <div class="mb-3">
                                    <label for="editTitle" class="form-label">Title</label>
                                    <input type="text" class="form-control" id="editTitle" value="Leaking Faucet">
                                </div>
                                <div class="mb-3">
                                    <label for="editStaff" class="form-label">Staff</label>
                                    <input type="text" class="form-control" id="editStaff" value="Juan Dela Cruz">
                                </div>
                                <div class="mb-3">
                                    <label for="editDate" class="form-label">Date</label>
                                    <input type="date" class="form-control" id="editDate" value="2023-10-01">
                                </div>
                                <div class="mb-3">
                                    <label for="editStatus" class="form-label">Status</label>
                                    <select class="form-select" id="editStatus">
                                        <option selected>Pending</option>
                                        <option>In Progress</option>
                                        <option>Completed</option>
                                    </select>
                                </div>
                                <div class="mb-3">
                                    <label for="editDescription" class="form-label">Description</label>
                                    <textarea class="form-control" id="editDescription" rows="3">The faucet in the kitchen is leaking continuously.</textarea>
                                </div>
                            </form>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                            <button type="button" class="btn btn-success">Save Changes</button>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Delete Modal -->
            <div class="modal fade" id="deleteModal" tabindex="-1" aria-labelledby="deleteModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="deleteModalLabel">Delete Request</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <p>Are you sure you want to delete <strong>REQ-001: Leaking Faucet</strong>?</p>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                            <button type="button" class="btn btn-danger">Delete</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    <!-- Create Request Modal -->
    <div class="modal fade" id="repairRequestModal" aria-hidden="true" aria-labelledby="repairRequestModalLabel" tabindex="-1">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header">
                    <h1 class="modal-title fs-5" id="repairRequestModalLabel">Create Repair Request</h1>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body text-center">
                    <p>Do you want to create a repair request?</p>
                </div>
                <div class="modal-footer justify-content-center">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">No</button>
                    <button type="button" class="btn btn-primary" data-bs-target="#newRequestModal" data-bs-toggle="modal">Yes</button>
                </div>
            </div>
        </div>
    </div>

